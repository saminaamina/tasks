using System;

namespace Raindrops
{
    class Program
    {
        static void Main(string[] args)
        {
            int amountOfRegions = int.Parse(Console.ReadLine());
            double density = double.Parse(Console.ReadLine());
            int RaindropsCount = 0;
            int squareMeters = 0; ;
            double regionalCoefficent = 0;
            for (int i = 0; i < amountOfRegions; i++)
            {
                RaindropsCount = int.Parse(Console.ReadLine());
                squareMeters = int.Parse(Console.ReadLine());
                regionalCoefficent = RaindropsCount / squareMeters;
            }
           Console.WriteLine(regionalCoefficent);
        }
    }
}
